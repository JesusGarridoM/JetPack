{
   "name": "CSP",
   "sample_ini": "sample_csp_profile.ini",
   "associated_settings": [
      {
         "Dell NFV Settings": {
            "hpg_enable": "",
            "hpg_size": {
               "valid_values": [
                  "2MB",
                  "1GB"
               ]
            },
            "numa_enable": "",
            "numa_hostos_cpu_count": {
                 "valid_values": [
                   "2",
                   "4",
                   "6",
                   "8"
                ]
             }
         },
         "Performance and Optimization": {
            "innodb_buffer_pool_instances": {
               "validate": [
                  "should_be_valid_int"
               ],
               "in_range": [
                  "1-48"
               ]
            },
            "innodb_buffer_pool_size": {
               "validate": [
                  "valid_buffer_pool_size"
               ]
            },
            "mariadb_max_connections": {
               "validate": [
                  "should_be_valid_int"
               ],
               "in_range": [
                  "1-100000"
               ]
            }
         }
      },
      {
         "Nodes Nics and Bonding Settings": {
            "sah_bond_opts": "",
            "nic_env_file": {
               "valid_values": [
                  "5_port/nic_environment.yaml"
               ]
            },
            "ControllerBondInterfaceOptions": "",
            "ComputeBondInterfaceOptions": "",
            "StorageBondInterfaceOptions": ""
         }
      },
      {
         "Deployment Settings":{
            "overcloud_static_ips" :{
               "valid_values": [
                  "true",
                  "false"
               ]
            },
         "enable_fencing":"",
         "enable_instance_ha":""
         }
      },
      {
         "Network Settings": {
            "public_api_gateway": {
               "validate": [
                  "should_be_valid_ip"
               ]
            },
            "public_api_allocation_pool_start": {
               "validate": [
                  "should_be_valid_ip"
               ]
            },
            "public_api_allocation_pool_end": {
               "validate": [
                  "should_be_valid_ip"
               ]
            }
         }
      }
   ]
}
